      ""m                        $m             $ O   A
         "m                     m" "m            R     R
           "mm                 $     $          G       B
              "m              $       "m       E        "A
                "m           $          "m    $          "m
                  ""m      m"             "m $""mmmm      $
                     "mm  m"
EOF
echo -e "\e[0m"
)
batman=$(echo -e "\e[38;5;11m"
cat << 'EOF'

              *         *      *         *
          ***          **********          ***
       *****           **********           *****
     *******           **********           *******
   **********         ************         **********
  ****************************************************
 ******************************************************
********************************************************
********************************************************
********************************************************
 ******************************************************
  ********      ************************      ********
   *******       *     *********      *       *******
     ******             *******              ******
       *****             *****              *****
          ***             ***              ***
            **             *              **
EOF

echo -e '\e[0m'
)
Redhat=$(echo -e '\e[38;5;196m'
cat << 'EOF'
                          _____
                   _.+sd$$$$$$$$$bs+._
               .+d$$$$$$$$$$$$$$$$$$$$$b+.
            .sd$$$$$$$P^*^T$$$P^*"*^T$$$$$bs.
          .s$$$$$$$$P*     `*' _._  `T$$$$$$$s.
        .s$$$$$$$$$P          ` :$;   T$$$$$$$$s.
       s$$$$$$$$$$;  db..+s.   `**'    T$$$$$$$$$s
     .$$$$$$$$$$$$'  `T$P*'             T$$$$$$$$$$.
    .$$$$$$$$$$$$P                       T$$$$$$$$$$.
   .$$$$$$$$$$$$$b                       `$$$$$$$$$$$.
  :$$$$$$$$$$$$$$$.                       T$$$$$$$$$$$;
  $$$$$$$$$P^*' :$$b.                     d$$$$$$$$$$$$
 :$$$$$$$P'      T$$$$bs._               :P'`*^T$$$$$$$;
 $$$$$$$P         `*T$$$$$b              '      `T$$$$$$
:$$$$$$$b            `*T$$$s                      :$$$$$;
:$$$$$$$$b.                                        $$$$$;
$$$$$$$$$$$b.                                     :$$$$$$
$$$$$$$$$$$$$bs.                                 .$$$$$$$
$$$$$$$$$$$$$$$$$bs.                           .d$$$$$$$$
:$$$$$$$$$$$$$P*"*T$$bs,._                  .sd$$$$$$$$$;
:$$$$$$$$$$$$P     TP^**T$bss++.._____..++sd$$$$$$$$$$$$;
 $$$$$$$$$$$$b           `T$$$$$$$$$$$$$$$$$$$$$$$$$$$$$
 :$$$$$$$$$$$$b.           `*T$$P^*"*"*^^*T$$$$$$$$$$$$;
  $$$b       `T$b+                        :$$$$$$$BUG$$
  :$P'         `"'               ,._.     ;$$$$$$$$$$$;
   \                            `*TP*     d$$P*******$
    \                                    :$$P'      /
     \                                  :dP'       /
      `.                               d$P       .'
        `.                             `'      .'
          `-.                               .-'
             `-.                         .-'
                `*+-._             _.-+*'
                      `"*-------*"'
EOF
echo -e '\e[0m'
)
menu_func () {
local options=${#@}
local array
for((num=01; num<=$options; num++)); do
echo -ne "\033[1;31m[\033[1;32m$num\033[1;31m]\033[0m\033[1;33m ▶ \033[0m"
  array=(${!num})
  case ${array[0]} in
    "-vd")echo -ne  "${array[@]:1}" | sed ':a;N;$!ba;s/\n/ /g';;
    "-vm")echo -ne  "${array[@]:1}" | sed ':a;N;$!ba;s/\n/ /g';;
    "-fi")echo -ne  "${array[@]:2} ${array[1]}" | sed ':a;N;$!ba;s/\n/ /g';;
    *)echo -e  "${array[@]}" | sed ':a;N;$!ba;s/\n/ /g';;
  esac
done
}
dibujo(){
echo "###########################"
menu_func "alien" "Minotaur" "silvestre" "tazmania" "Bob Esponja" "cartman" "coyote" "boy" "python" "homero" "batman" "Redhat" "cráneo" "kyle" "stan" "dog"
echo "###########################"
echo -ne "\033[1;36mSelecione una Opcion: \033[0m" >&2
read selection
case ${selection} in
1)echo -e "$alien" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
2)echo -e "$Minotaur" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
3)echo -e "$silvestre" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
4)echo -e "$tazmania" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
5)echo -e "$bob" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
6)echo -e "$cartman" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
7)echo -e "$coyote" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
8)echo -e "$boy" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
9)echo -e "$python" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
10)echo -e "$homero" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
11)echo -e "$batman" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
12)echo -e "$Redhat" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
13)echo -e "$craneo" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
14)echo -e "$kyle" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
15)echo -e "$stan" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
16)echo -e "$dog" >> /data/data/com.termux/files/usr/etc/cl34r.txt ;;
esac
}
clear
[[ -e /data/data/com.termux/files/usr/etc/cl34r.txt ]] && rm /data/data/com.termux/files/usr/etc/cl34r.txt
[[ -e /data/data/com.termux/files/usr/etc/bash.bashrc ]] && rm /data/data/com.termux/files/usr/etc/bash.bashrc
echo -e "PS1='$'" > /data/data/com.termux/files/usr/etc/bash.bashrc
dibujo
echo "clear" >> /data/data/com.termux/files/usr/etc/bash.bashrc
echo "cat /data/data/com.termux/files/usr/etc/cl34r.txt" >> /data/data/com.termux/files/usr/etc/bash.bashrc
echo -e "date +'%d/%m/%Y %H:%M' | lolcat -d -a" >> /data/data/com.termux/files/usr/etc/bash.bashrc

